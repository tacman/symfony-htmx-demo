{
    "name": "survos-sites/htmx-demo",
    "description": "a demo of HTMX from the Symfony tutorial",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "nadar/php-composer-reader": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.24",
        "survos/doc-bundle": "^1.5",
        "symfony/asset": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/emoji": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/flex": "^2",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/intl": "^7.0",
        "symfony/mime": "^7.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/process": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/web-link": "^7.0",
        "symfony/yaml": "^7.0",
        "symfonycasts/tailwind-bundle": "*",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "jawira/doctrine-diagram-bundle": "*",
        "nette/php-generator": "^4.1",
        "phpunit/phpunit": "^12.1",
        "survos/code-bundle": "dev-main",
        "survos/crawler-bundle": "^1.5",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "^7.0",
        "symfony/web-profiler-bundle": "^7.0",
        "zenstruck/browser": "^1.9",
        "zenstruck/console-test": "^1.7"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^7.3"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts",
            "php bin/console tailwind:build"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "php bin/console tailwind:build"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "diagram": "mkdir -p assets/images && bin/console doctrine:diagram --filename=assets/images/db.svg --exclude=doctrine_migration_versions,messenger_messages"
    }
}
